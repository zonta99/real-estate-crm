# Real Estate CRM - Environment Variables
# Copy this file to .env and fill in the values
# NEVER commit .env file to version control

# JWT Configuration
# Generate a strong secret with: openssl rand -base64 64
JWT_SECRET=your-super-secret-jwt-key-here-minimum-256-bits-use-openssl-rand-base64-64
JWT_EXPIRATION=86400000

# Admin Initial Credentials (change immediately after first login)
ADMIN_USERNAME=admin
ADMIN_PASSWORD=ChangeThisPasswordImmediately!

# Database Configuration (for production)
# DB_URL=jdbc:postgresql://localhost:5432/realestatecrm
# DB_USERNAME=dbuser
# DB_PASSWORD=dbpassword

# CORS Allowed Origins (comma-separated)
CORS_ALLOWED_ORIGINS=http://localhost:3000,http://localhost:4200

# Server Configuration
# SERVER_PORT=8080
